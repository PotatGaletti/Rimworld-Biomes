</ThingDef>
<ThingDef ParentName="PlantBase">
<defName>PlantChestnut</defName>
<label>Chestnut</label>
<description>Beautiful, long-lived trees that grow to massive heights. They produce a very large mast each year, providing delicious nuts for animals and humans alike. </description>
<altitudeLayer>Building</altitudeLayer>
<statBases>
<MaxHitPoints>200</MaxHitPoints>
</statBases>
<graphicData>
<texPath>Things/Plant/Chestnut/Chestnutfall</texPath>
<graphicClass>Graphic_Random</graphicClass>
<shadowData>
<volume>(0.3, 0.3, 0.3)</volume>
</shadowData>
</graphicData>
<passability>PassThroughOnly</passability>
<blockWind>true</blockWind>
<selectable>true</selectable>
<fillPercent>0.35</fillPercent>
<pathCost>80</pathCost>
<ingestible>
<preferability>RawBad</preferability>
<nutrition>1.0</nutrition>
</ingestible>
<plant>
<growDays>25</growDays>
<sowWork>1500</sowWork>
<harvestWork>1000</harvestWork>
<harvestTag>Standard</harvestTag>
<harvestDestroys>false</harvestDestroys>
<harvestedThingDef>ChestnutsProduct</harvestedThingDef>
<harvestYield>20</harvestYield>
<harvestMinGrowth>0.40</harvestMinGrowth>
<harvestFailable>false</harvestFailable>
<wildCommonalityMaxFraction>3.0</wildCommonalityMaxFraction>
<blockAdjacentSow>true</blockAdjacentSow>
<sowTags/>
<visualSizeRange>
<min>1.8</min>
<max>3.4</max>
</visualSizeRange>
<fertilityMin>0.3</fertilityMin>
<topWindExposure>0.25</topWindExposure>
<fertilitySensitivity>0.5</fertilitySensitivity>
<wildClusterSizeRange>
<min>8</min>
<max>30</max>
</wildClusterSizeRange>
<wildClusterRadius>8</wildClusterRadius>
</plant>
</ThingDef>

