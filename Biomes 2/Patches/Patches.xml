<?xml version="1.0" encoding="UTF-8"?>

<Patch>


	<Operation Class="PatchOperationReplace"> <!-- Start of Biome modification -->
		<xpath>/Defs/BiomeDef[defName = "TemperateForest"]/animalDensity</xpath>
		<value>
			<animalDensity>3.5</animalDensity>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/BiomeDef[defName = "TemperateForest"]/plantDensity</xpath>
		<value>
			<plantDensity>1.0</plantDensity>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/BiomeDef[defName = "TemperateForest"]/wildPlants</xpath>
		<value>
			<wildPlants>

				<PlantHuckleberryBush>0.3</PlantHuckleberryBush>
				<PlantMorel>0.1</PlantMorel>
				<PlantTrillium>0.6</PlantTrillium>
				<PlantGoldenrod>0.7</PlantGoldenrod>
				<PlantWildGarlic>0.2</PlantWildGarlic>
				<PlantChanterelle>0.1</PlantChanterelle>
				<PlantSpringBeauty>0.5</PlantSpringBeauty>
				<PlantChestnut>0.5</PlantChestnut>
				<PlantMaple>0.5</PlantMaple>
				<PlantAmaranth>0.2</PlantAmaranth>
				<PlantYellowBirch>0.5</PlantYellowBirch>
				<PlantOakAOrange>0.5</PlantOakAOrange>
				<PlantOakBOrange>0.5</PlantOakBOrange>
		    </wildPlants>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd"> <!-- End of Biome modification -->
		<xpath>/Defs/BiomeDef[defName = "TemperateForest"]/wildAnimals</xpath>
		<value>
			<wildAnimals>
				<LandSnail>0.7</LandSnail>
			</wildAnimals>
		</value>
	</Operation>

</Patch>


